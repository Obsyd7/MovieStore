@model IEnumerable<MovieStore.Models.MovieModel>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Movies</h2>
<table class="table">
    <tr>
        <th>
            Movie
        </th>
        <th>
            Genre
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>

            <td>
                @item.Genre.Name
            </td>
        </tr>
    }
</table>